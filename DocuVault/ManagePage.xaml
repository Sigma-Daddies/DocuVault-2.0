<Page x:Class="DocuVault.ManagePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="880" d:DesignWidth="985" Background="Yellow" Margin="0, 0, 0, 0"
      Title="Manage Files">

    <Grid x:Name="Upload_Btn">
        <DataGrid x:Name="Documents_DataGrid" Margin="80,128,80,140"  />
        <Button x:Name="UploadButton" Content="Upload" HorizontalAlignment="Left" Margin="638,753,0,0" VerticalAlignment="Top" Click="UploadButton_Click"/>
        <Button x:Name="DeleteButton" Content="Delete" HorizontalAlignment="Left" Margin="711,753,0,0" VerticalAlignment="Top" Click="DeleteButton_Click"/>
        <Button x:Name="DownloadButton" Content="Download" HorizontalAlignment="Left" Margin="789,753,0,0" VerticalAlignment="Top" Click="DownloadButton_Click"/>

    </Grid>
</Page>
